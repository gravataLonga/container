{
    "name": "gravatalonga/container",
    "type": "library",
    "description": "Container is a library of implement of container following PSR-11",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Fontes",
            "email": "jonathan.alexey16@gmail.com",
            "homepage": "https://jonathan.pt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 7.1.3",
        "psr/container": "^1.0"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {
        "drupol/php-conventions": "^1.7",
        "infection/infection": "^0.13.6 || ^0.15.0",
        "phpunit/phpunit": "^7 || ^8",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Gravatalonga\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "grumphp": "grumphp run"
    }
}
